{
  "name": "gpt",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.14.0",
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm install --prefer-offline --loglevel warn",
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:visual:ci": "playwright test --config=playwright-visual.config.ts tests/visual/homepage.visual.spec.ts",
    "test:visual:ui": "playwright test --config=playwright-visual.config.ts --ui",
    "test:visual:debug": "playwright test --config=playwright-visual.config.ts --debug",
    "test:visual:update": "playwright test --config=playwright-visual.config.ts --update-snapshots",
    "test:visual:headed": "playwright test --config=playwright-visual.config.ts --headed",
    "test:accessibility": "playwright test tests/accessibility/",
    "test:accessibility:ui": "playwright test tests/accessibility/ --ui",
    "test:accessibility:debug": "playwright test tests/accessibility/ --debug",
    "test:accessibility:headed": "playwright test tests/accessibility/ --headed",
    "fix": "pnpm run lint --fix",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@bfra.me/prettier-config": "0.16.1",
    "@heroui/react": "2.8.2",
    "@langchain/anthropic": "0.3.26",
    "@langchain/core": "0.3.66",
    "@langchain/langgraph": "0.4.5",
    "@langchain/openai": "0.6.3",
    "@monaco-editor/react": "4.7.0",
    "@react-aria/ssr": "3.9.10",
    "@types/uuid": "10.0.0",
    "clsx": "2.1.1",
    "framer-motion": "12.23.12",
    "langchain": "0.3.30",
    "lru-cache": "11.1.0",
    "lucide-react": "0.533.0",
    "next-themes": "0.4.6",
    "openai": "5.12.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router-dom": "7.8.0",
    "uuid": "11.1.0",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.2",
    "@bfra.me/eslint-config": "0.26.0",
    "@eslint-react/eslint-plugin": "1.52.3",
    "@playwright/test": "1.54.2",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.11",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@tsconfig/node20": "20.1.6",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.16.5",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react-swc": "3.11.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.3.4",
    "autoprefixer": "10.4.21",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-hooks": "canary",
    "eslint-plugin-react-refresh": "0.4.20",
    "jiti": "2.4.2",
    "jsdom": "26.1.0",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "vite": "7.1.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "pnpm": {
    "overrides": {
      "jiti": "<2.5.0"
    },
    "onlyBuiltDependencies": [
      "@heroui/shared-utils",
      "@swc/core",
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "prettier": "@bfra.me/prettier-config/120-proof"
}
